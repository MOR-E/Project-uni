<div class="container">
    <hr>
    <div class="card mt-5">
        <div class="card-header">
            <h2>Criação de usuario</h2>
        </div>
        <div class="card-body">
            <form method="post" action="/usuario/criar">
                <input type="email" name="email" class="form-control" placeholder="email@exemplo.com"><br>
                <input type="password" name="senha" class="form-control" placeholder="******"><br>
                <button type="submit" class="btn btn-success btn-block">Criar</button>
            </form>
        </div>
    </div>
</div>



